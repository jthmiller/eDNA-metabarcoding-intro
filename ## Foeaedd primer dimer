## Foeaedd primer dimer


## coi
    fw=GGWACWGGWTGAACWGTWTAYCCYCC
    rv=TANACYTCNGGRTGNCCRAARAAYCA

## AC16
    fw=CCTTTTGCATCATGATTTAGC
    rv=CAGGTGGCTGCTTTTAGGC

    ## dimer? 
    CCTTTTGCATCATGATTTAGCCTAAAAGCAGCCACATGCTGTCTCTTATACACATCTCCGAGCCCACGAGACTGATAGGCATCGCGTATGCCGTCTTCTGCTTGAAAAGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGG



echo "file ac16 coi dimer" > primer_counts.txt

for file in *R1_001.fastq.gz ; do 
    ac16=$( zgrep ^CCTTTTGCATCATGATTTAGC $file | wc -l)
    coi=$( zgrep "^GG.AC.GG.TGAAC.GT.TA.CC.CC" $file | wc -l)
    dimer=$( zgrep GGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGG $file | wc -l)
    echo -e "$file $ac16 $coi $dimer"
done >> primer_counts.txt



echo "file coi dimer" > primer_counts.txt

for file in *R1_001.fastq.gz ; do 
    coi=$( zgrep "^GG.AC.GG.TGAAC.GT.TA.CC.CC" $file | wc -l)
    dimer=$( zgrep GGGGGGGGGGGG $file | wc -l)
    echo -e "$file $coi $dimer"
done >> primer_counts.txt




## mifish
    fw=GTCGGTAAAACTCGTGCCAGC	
    rv=CATAGTGGGGTATCTAATCCCAGTTTG


for file in *R1_001.fastq.gz ; do 
    coi=$( zgrep "GTCGGTAAAACTCGTGCCAGC.*" $file | wc -l)
    dimer=$( zgrep GGGGGGGGGGGG $file | wc -l)
    echo -e "$file $coi $dimer"
done >> primer_counts.txt




